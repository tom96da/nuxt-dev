{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "nuxt typecheck",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "i18n:report": "vue-i18n-extract report --vueFiles './src/**/*.?(ts|vue)' --languageFiles './src/i18n/locales/*.?(json|yml|yaml|ts)' --add --remove"
  },
  "dependencies": {
    "@nuxt/content": "3.6.3",
    "@nuxt/eslint": "1.9.0",
    "@nuxt/image": "1.11.0",
    "@nuxt/ui": "4.0.0-alpha.0",
    "@nuxtjs/i18n": "10.0.6",
    "better-sqlite3": "^12.2.0",
    "consola": "^3.4.2",
    "eslint": "^9.34.0",
    "http-status-codes": "^2.3.0",
    "nuxt": "^4.0.3",
    "tailwindcss": "^4.1.12",
    "typescript": "^5.9.2",
    "uuid": "^11.1.0",
    "vue": "^3.5.20",
    "vue-router": "^4.5.1",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@nuxt/test-utils": "3.19.2",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-n": "^17.21.3",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-unicorn": "^60.0.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "vue-i18n-extract": "^2.0.7"
  },
  "engines": {
    "pnpm": "10.15.0",
    "npm": "please_use_pnpm_instead"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3",
      "@tailwindcss/oxide",
      "vue-demi"
    ]
  },
  "packageManager": "pnpm@10.15.0"
}
